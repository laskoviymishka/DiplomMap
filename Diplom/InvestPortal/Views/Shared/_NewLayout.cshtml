<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <script src="http://localhost:53235/Scripts/jquery-1.10.2.min.js" type="text/javascript"></script>
    <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=true"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/jquery.ui.widget.js" type="text/javascript"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/metro-treeview.js" type="text/javascript"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/metro-accordion.js" type="text/javascript"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/metro-button-set.js" type="text/javascript"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/metro-calendar.js" type="text/javascript"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/metro-carousel.js" type="text/javascript"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/metro-core.js" type="text/javascript"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/metro-countdown.js" type="text/javascript"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/metro-date-format.js" type="text/javascript"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/metro-datepicker.js" type="text/javascript"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/metro-dialog.js" type="text/javascript"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/metro-drag-tile.js" type="text/javascript"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/metro-dropdown.js" type="text/javascript"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/metro-fluentmenu.js" type="text/javascript"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/metro-hint.js" type="text/javascript"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/metro-input-control.js" type="text/javascript"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/metro-listview.js" type="text/javascript"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/metro-live-tile.js" type="text/javascript"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/metro-notify.js" type="text/javascript"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/metro-plugin-template.js" type="text/javascript"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/metro-progressbar.js" type="text/javascript"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/metro-rating.js" type="text/javascript"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/metro-slider.js" type="text/javascript"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/metro-stepper.js" type="text/javascript"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/metro-streamer.js" type="text/javascript"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/metro-tab-control.js" type="text/javascript"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/metro-table.js" type="text/javascript"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/metro-times.js" type="text/javascript"></script>
    <script src="http://localhost:53235/Scripts/metro-ui/metro-touch-handler.js" type="text/javascript"></script>
    <link href="http://localhost:53235/Content/metro-ui/css/metro-bootstrap-responsive.css"
        rel="stylesheet" type="text/css" />
    <link href="http://localhost:53235/Content/metro-ui/css/metro-bootstrap.css" rel="stylesheet"
        type="text/css" />
    <script src="~/Scripts/gmaps.js"></script>
    <body class="metro">
        <div class="grid">
            <nav class="navigation-bar bd-lightBlue">
                <nav class="navigation-bar-content">
                    <div class="element">
                        <a class="dropdown-toggle" href="#">METRO UI CSS</a>
                        <ul class="dropdown-menu" data-role="dropdown">
                            <li><a href="#">Main</a></li>
                            <li><a href="#">File Open</a></li>
                            <li class="divider"></li>
                            <li><a href="#">Print...</a></li>
                            <li class="divider"></li>
                            <li><a href="#">Exit</a></li>
                        </ul>
                    </div>
 
                    <span class="element-divider"></span>
                    <a class="element brand" href="#"><span class="icon-spin"></span></a>
                    <a class="element brand" href="#"><span class="icon-printer"></span></a>
                    <span class="element-divider"></span>
 
                    <div class="element input-element">
                        <form>
                            <div class="input-control text">
                                <input type="text" placeholder="Search...">
                                <button class="btn-search"></button>
                            </div>
                        </form>
                    </div>
 
                    <div class="element place-right">
                        <a class="dropdown-toggle" href="#">
                            <span class="icon-cog"></span>
                        </a>
                        <ul class="dropdown-menu place-right" data-role="dropdown">
                            <li><a href="#">Products</a></li>
                            <li><a href="#">Download</a></li>
                            <li><a href="#">Support</a></li>
                            <li><a href="#">Buy Now</a></li>
                        </ul>
                    </div>
                    <span class="element-divider place-right"></span>
                    <a class="element place-right" href="#"><span class="icon-locked-2"></span></a>
                    <span class="element-divider place-right"></span>
                    <button class="element image-button image-left place-right">
                        @User.Identity.Name
                    </button>
                </nav>
            </nav>
            <div class="row">
                <div class="span4">
                 @{Html.RenderAction("ProfilePartial", "Profile");}
                    <ul class="dropdown-menu open keep-open" style="position: relative; z-index: 1">
                        <li class="menu-title">This is a title</li>
                        <li><a href="#">Action</a></li>
                        <li><a href="#">Another action</a></li>
                        <li class="menu-title">This is another title</li>
                        <li><a href="#">Something else here</a></li>
                        <li class="divider"></li>
                        <li class="disabled"><a href="#">Disabled link</a></li>
                        <li class="divider"></li>
                        <li class="checked"><a href="#">Checked link</a></li>
                    </ul>
                </div>
                <div class="span8">
                    @RenderBody()
                </div>
            </div>
        </div>
    </body>
</html>
