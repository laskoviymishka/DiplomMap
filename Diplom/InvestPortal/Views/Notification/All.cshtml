@model IEnumerable<Invest.Common.Model.User.NotificationQueue>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NotificationTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NotigicationBody)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsRead)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NotificationTime)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NotificationTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NotigicationBody)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsRead)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NotificationTime)
        </td>
        <td>
            @Html.ActionLink("Прочитать", "Read", new { id = item._id}) 
        </td>
    </tr>
}

</table>
