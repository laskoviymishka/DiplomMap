@model Investmogilev.Infrastructure.Common.Model.Project.BrownField

<fieldset>
    <legend>Brown Field - инвестиция в существущее производство</legend>
    <table class="table table-striped table-condensed">
        <tr>
            <td>Название проекта</td>
            <td>@Html.DisplayFor(model => model.Name)</td>
        </tr>
        <tr>
            <td>Описание проекта</td>
            <td>@Html.Raw(HttpUtility.HtmlDecode(Model.Description))</td>
        </tr>
    </table>
    <a id="popupLink" href="/InvestorEntry/ResponseToProject/@Model._id" class="button button success">Откликнуться на проект</a>
</fieldset>